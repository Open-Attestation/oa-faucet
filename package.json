{
  "name": "faucet-api",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=offline sls offline start --host 0.0.0.0",
    "dev:test": "NODE_ENV=offline sls offline start",
    "depcheck": "depcheck .",
    "sls-config-check": "sls print",
    "test": "NODE_ENV=offline jest --testPathPattern=src",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:integration": "NODE_ENV=offline jest --testPathPattern=test/integration",
    "test:e2e": "NODE_ENV=offline jest --testPathPattern=test/e2e --runInBand --detectOpenHandles",
    "test:all": "NODE_ENV=offline npm run test && npm run test:integration && npm run test:e2e",
    "test:ci": "concurrently -s first -k -n sls-offline,test-suite \"NODE_ENV=offline sls offline start\" \"wait-on http://localhost:8000/shell && npm run test:all && npm run plugins:test\"",
    "lint": "eslint . --ext ts,js --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "rjchow",
  "license": "ISC",
  "dependencies": {
    "@middy/core": "^1.4.0",
    "@middy/http-cors": "^1.4.0",
    "debug": "^4.2.0",
    "ethers": "^5.0.31",
    "ethers-aws-kms-signer": "^1.1.0",
    "http-errors": "^1.8.0",
    "middy": "^0.36.0",
    "runtypes": "^5.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/aws-lambda": "^8.10.64",
    "@types/debug": "^4.1.5",
    "@types/http-errors": "^1.8.0",
    "@types/jest": "^26.0.15",
    "@types/serverless": "^1.78.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.7.0",
    "aws-sdk": "2.712.0",
    "concurrently": "^5.3.0",
    "depcheck": "^1.3.1",
    "dotenv": "^8.2.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "jest": "^26.6.0",
    "prettier": "^2.1.2",
    "serverless": "^2.9.0",
    "serverless-dotenv-plugin": "^3.1.0",
    "serverless-offline": "6.8.0",
